{
  "name": "log-editor",
  "version": "0.1.0",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./console": {
      "require": "./dist/console.js",
      "import": "./dist/console.mjs"
    }
  },
  "funding": "https://github.com/sponsors/antfu",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/antfu/log-editor/issues"
  },
  "homepage": "https://github.com/antfu/log-editor#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/log-editor.git"
  },
  "keywords": [],
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "nr build",
    "dev": "nr build --watch",
    "start": "esno src/index.ts",
    "build": "tsup src/index.ts src/console.ts --format cjs,esm --dts --no-splitting",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "lint:fix": "nr lint -- --fix"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.6.2",
    "@antfu/ni": "^0.5.5",
    "@types/node": "^14.14.37",
    "bumpp": "^6.0.6",
    "eslint": "^7.23.0",
    "esno": "^0.5.0",
    "tsup": "^4.8.21",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "launch-editor": "^2.2.1",
    "tempy": "^1.0.1"
  }
}
